    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
<style>
    h1{
        text-decoration: none;
    }
    #fg{
    margin-top: 100px;
    margin-left: 600px;
        text-align: center;
        border: 2px solid;
        border-width: 10px;
        border-radius: 10px;
        width:fit-content;
        height:fit-content;
        box-shadow: 3px;
    }
    box{
        margin-top: 50px;
        margin-left: 50px;
        text-align: center;
       
  
        
    }
    
    table{
        font-style: bold;
        font-size: 100px;
        font-size: large;
        
        
        text-align: center;
        
    }
.a{
    font-style: normal;
}
    
    
</style>
<script >

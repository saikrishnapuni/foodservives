    //         a.document.write('</body></html>');
    
    // a.print();
    // }
</script>
</head>
<body>
    <div id = "fg">
    <box>
       
    <h1>BILL</h1>
    <table>
        <%var dict = [];%>
        <%var p = [];%>
        <tr>
            <td>ITEM</td>
            <td><h1></h1></td>
            <td class="a">QUANTITY</td>
            <td><h1>                                    </h1></td>
            <td>PRICE</td>
        </tr>
        <% data.forEach(function(d){%>
            <tr>
                
                <%if(sampleData[d.SNO]!=0 ){%>
                    <%p.push(d.PRICE)%>
                    <%dict.push({key:d.SNO,value:sampleData[d.SNO]});%>
                <td><%=d.ITEM%></td>
                <td><h1></h1></td>
                <td class="a"><%=sampleData[d.SNO]%></td>
                
                <td><h1>                                    </h1></td>
                <td><%=d.PRICE%></td>
                
                <%}%>
                    <%});%>
            </tr>
        </table>
        <%sum = 0%>
        
        <%for(let i=0;i<dict.length;i++){%>
            
            <%=console.log((p[i]*(dict[i].value)))%>
            <%sum = sum+(p[i]*(dict[i].value))%>
            <%};%>
        

    <h2 class="a">Your total bill is  = <%=sum%> </h1>
        
</box>

</div>
<button onclick="f()">click</button>
